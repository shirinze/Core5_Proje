@model List<EntityLayer.Concreate.Experience>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Experience List</h4>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Title</th>
                            <th>Date </th>
                            <th></th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.ExperienceID</td>
                                <td>@item.Name</td>
                                <td>@item.Date</td>
                                <td><a href="/Experience/ExperienceDelete/@item.ExperienceID" class="btn btn-outline-danger">Delete</a></td>
                                <td><a href="/Experience/ExperienceUpdate/@item.ExperienceID" class="btn btn-outline-warning">Update</a></td>
                            </tr>
                        }
                    </tbody>
                </table>
                <a href="/Experience/ExperienceAdd" class="btn btn-outline-info">NEW EXPERIENCE</a>
            </div>
        </div>
    </div>
</div>
<script src="~/corona-free-dark-bootstrap-admin-template-1.0.0/template/assets/vendors/js/vendor.bundle.base.js"></script>


